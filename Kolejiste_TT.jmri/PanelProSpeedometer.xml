<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="/xml/XSLT/speedometer.xsl" type="text/xsl"?>
<speedometer-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://jmri.org/xml/schema/speedometer-3-9-3.xsd">
  <configuration>
    <useMetric>no</useMetric>
  </configuration>
  <sensors>
    <sensor>
      <sensorName>S16</sensorName>
      <type>StartSensor</type>
      <trigger>entry</trigger>
    </sensor>
    <sensor>
      <sensorName>S03</sensorName>
      <type>StopSensor1</type>
      <trigger>entry</trigger>
      <distance>55.0</distance>
    </sensor>
    <sensor>
      <sensorName>S25</sensorName>
      <type>StopSensor2</type>
      <trigger>entry</trigger>
      <distance>177.0</distance>
    </sensor>
  </sensors>
</speedometer-config>
